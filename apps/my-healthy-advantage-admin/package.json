{
    "name": "my-healthy-advantage-admin",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.13",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "customize-cra": "^1.0.0",
        "react": "^18.2.0",
        "react-app-rewired": "^2.2.1",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "tailwindcss": "^3.2.7",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4",
        "wrangler": "^2.12.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "*.test.tsx",
                    "*.spec.tsx"
                ],
                "rules": {
                    "testing-library/no-render-in-setup": [
                        "error",
                        {
                            "allowTestingFrameworkSetupHook": "beforeEach"
                        }
                    ]
                }
            }
        ]
    },
    "jest": {
        "moduleNameMapper": {
            "shared/(.*)": "<rootDir>/../shared/src/$1"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
